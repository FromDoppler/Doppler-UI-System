{
  "name": "Doppler-UI",
  "private": true,
  "version": "0.1.1",
  "description": "UI library for easy inclusion css, components, images and fonts in projects",
  "homepage": "https://fromdoppler.com",
  "keywords": [
    "ui",
    "ui library"
  ],
  "author": "Doppler UI Team",
  "contributors": [
    {
      "name": "Ignacio Rodrigues",
      "email": "irodrigues@fromdoppler.com"
    },
    {
      "name": "Mariana Massimino",
      "email": "mmassimino@fromdoppler.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FromDoppler/Doppler-UI-System"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "prettier": "prettier \"assets/**/*.{css,scss,html}\" \"!assets/scss/libs/**/*.{css,scss}\" --write",
    "prettier-check": "prettier-check \"assets/**/*.{css,scss,html}\" \"!assets/scss/libs/**/*.{css,scss}\"",
    "eclint-check": "./node_modules/.bin/eclint check \"**/*\""
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "devDependencies": {
    "@semantic-release/exec": "^5.0.0",
    "autoprefixer": "^9.8.0",
    "browser-sync": "^2.26.7",
    "css-mqpacker": "~7.0.0",
    "del": "^5.1.0",
    "eclint": "^2.8.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "~2.6.1",
    "gulp-csso": "^4.0.1",
    "gulp-iconfont": "^10.0.3",
    "gulp-iconfont-css": "~3.0.0",
    "gulp-imagemin": "~7.1.0",
    "gulp-kraken": "~0.2.0",
    "gulp-postcss": "~8.0.0",
    "gulp-prettier": "^3.0.0",
    "gulp-processhtml": "~1.2.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sftp": "~0.1.5",
    "gulp-sourcemaps": "~2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "~3.0.8",
    "gulp-zip": "~5.0.1",
    "node-sass": "^4.14.1",
    "postcss-clean": "~1.1.0",
    "postcss-flexibility": "^2.0.0",
    "prettier": "^2.0.5",
    "prettier-check": "^2.0.0",
    "sassdoc": "^2.7.1",
    "semantic-release": "^17.0.7",
    "sort-css-media-queries": "^1.5.0",
    "ttf2woff2": "^2.0.3",
    "web-push": "^3.2.2",
    "express": "^4.17.1"
  },
  "main": "pdr",
  "engines": {
    "node": ">=12.16.3",
    "npm": ">=6.14.4"
  },
  "dependencies": {},
  "release": {
    "branch": "master",
    "dryRun": false,
    "noCi": true,
    "debug": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "analyzeCommits": [
      "@semantic-release/commit-analyzer"
    ],
    "generateNotes": [
      "@semantic-release/release-notes-generator"
    ],
    "prepare": [
      {
        "path": "@semantic-release/exec",
        "__comment": "Running compile-prod here allow us to inject release version if need.",
        "cmd": "echo ${nextRelease.version} > dist/version.txt && export pkgVersion=v${nextRelease.version} && gulp dist"
      }
    ],
    "publish": [
      {
        "path": "@semantic-release/github"
      }
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  }
}
